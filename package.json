{
  "name": "CODAP-mcp",
  "version": "0.0.1",
  "description": "Concord Consortium Starter CODAP Plugin",
  "main": "index.js",
  "browserslist": "> 0.2%, last 5 versions, Firefox ESR, not dead, not ie > 0",
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setupTests.ts"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/playwright/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/codap-plugin-api-main/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(pkce-challenge|@modelcontextprotocol)/)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "npm run build:webpack",
    "build:webpack": "webpack --mode production",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:playwright": "npx playwright test",
    "lint": "npm run lint:check",
    "lint:check": "eslint . --config eslint.config.mjs",
    "lint:fix": "eslint . --config eslint.config.mjs --fix",
    "lint:style": "eslint . --config eslint.style.config.mjs",
    "lint:build": "eslint . --config eslint.build.config.mjs",
    "type-check": "tsc --noEmit",
    "deploy": "vercel --prod",
    "alias:update": "vercel alias set $(vercel ls --scope=cdorsey-concordorgs-projects | grep codap-mcp | head -1 | awk '{print $3}') codap-mcp-stable.vercel.app",
    "deploy:gh-pages": "npm run build && npx gh-pages -d dist",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/starter-projects.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/starter-projects/issues"
  },
  "homepage": "https://github.com/concord-consortium/starter-projects#readme",
  "devDependencies": {
    "@bgotink/playwright-coverage": "^0.3.2",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/js": "^9.22.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@playwright/test": "^1.51.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@stylistic/eslint-plugin-jsx": "^4.2.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/cors": "^2.8.19",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.32",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "autoprefixer": "^10.4.21",
    "caniuse-lite": "^1.0.30001706",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.22.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-webpack-plugin": "^5.0.0",
    "eventsource-parser": "^3.0.2",
    "file-loader": "^6.2.0",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-lib-coverage": "^3.2.2",
    "jest": "^29.7.0",
