    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.1.1",
    "rollup-plugin-dts": "^6.2.1",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "script-loader": "^0.7.2",
    "style-loader": "^4.0.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1",
    "wait-on": "^8.0.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@concord-consortium/codap-plugin-api": "^0.1.9",
    "@modelcontextprotocol/sdk": "^1.12.3",
    "@upstash/redis": "^1.35.0",
    "@vercel/kv": "^3.0.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "eventsource": "^4.0.0",
    "express": "^4.18.2",
    "iframe-phone": "^1.3.1",
    "node-fetch": "^2.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tslib": "^2.8.1",
    "zod": "^3.25.64"
  },
  "workspaces": [
    "claude-extension"
  ]
}
